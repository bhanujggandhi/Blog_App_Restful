<%- include("partials/header") %> 
<h1>INDEX PAGE!!!</h1>

<% for(var i = 0; i < blogs.length; i++){ %> 
    <div>
        <h2><%= blogs[i].title %> </h2>
        <img src="<%= blogs[i].image %>" alt="">
        <span><%= blogs[i].created %> </span>
        <p><%= blogs[i].body.substring(0, 100) %>....</p>
        <a href="/blogs/<%= blogs[i]._id %> ">Read More</a>
    </div>
<% } %>

<%- include("partials/footer") %> 